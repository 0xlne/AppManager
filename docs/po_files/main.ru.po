msgid "AppOpsService"
msgstr ""

#. type: section{#2}
#: ./appendices/app_ops.tex
#: :179
msgid "appops.xml"
msgstr ""

#. type: document
#: ./appendices/app_ops.tex
#: :181
msgid ""
"Latest \\texttt{appops.xml} has the following format: (This DTD is made by "
"me and by no means perfect, has compatibility issues.)"
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :184
msgid "<!DOCTYPE app-ops ["
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :187
msgid "<!ELEMENT app-ops (uid|pkg)*> <!ATTLIST app-ops v CDATA #IMPLIED>"
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :190
msgid "<!ELEMENT uid (op)*> <!ATTLIST uid n CDATA #REQUIRED>"
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :193
msgid "<!ELEMENT pkg (uid)*> <!ATTLIST pkg n CDATA #REQUIRED>"
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :198
msgid "<!ELEMENT uid (op)*> <!ATTLIST uid n CDATA #REQUIRED p CDATA #IMPLIED>"
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :203
msgid "<!ELEMENT op (st)*> <!ATTLIST op n CDATA #REQUIRED m CDATA #REQUIRED>"
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :212
msgid ""
"<!ELEMENT st EMPTY> <!ATTLIST st n CDATA #REQUIRED t CDATA #IMPLIED r CDATA "
"#IMPLIED d CDATA #IMPLIED pp CDATA #IMPLIED pu CDATA #IMPLIED>"
msgstr ""

#. type: Verbatim
#: ./appendices/app_ops.tex
#: :215
msgid "]>"
msgstr ""

#. type: document
#: ./appendices/app_ops.tex
#: :222
msgid "The instruction below follows the exact order given above:"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :222
msgid ""
"\\texttt{app-ops}: The root element. It can contain any number of "
"\\texttt{pkg} or package \\texttt{uid}"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :222
msgid ""
"\\texttt{v}: (optional, integer) The version number (default: "
"\\texttt{NO\\_VERSION} or \\texttt{-1})"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :227
msgid ""
"\\texttt{pkg}: Stores package info. It can contain any number of "
"\\texttt{uid}"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :227
msgid "\\texttt{n}: (required, string) Name of the package"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :232
msgid "Package \\texttt{uid}: Stores package or packages info"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :232 ./appendices/app_ops.tex
#: :238
msgid "\\texttt{n}: (required, integer) The user ID"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :238
msgid ""
"\\texttt{uid}: The package user ID. It can contain any number of "
"\\texttt{op}"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :238
msgid "\\texttt{p}: (optional, boolean) Is the app is a private/system app"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :244
msgid "\\texttt{op}: The operation, can contain \\texttt{st} or nothing at all"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :244
msgid ""
"\\texttt{n}: (required, integer) The op name in integer, i.e.\\ "
"AppOpsManager.OP\\_*"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :244
msgid "\\texttt{m}: (required, integer) The op mode, i.e.\\ AppOpsManager.MODE\\_*"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :255
msgid ""
"\\texttt{st}: State of operation: whether the operation is accessed, "
"rejected or running (not available on old versions)"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :255
msgid "\\texttt{n}: (required, long) Key containing flags and uid"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :255
msgid "\\texttt{t}: (optional, long) Access time (default: \\texttt{0})"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :255
msgid "\\texttt{r}: (optional, long) Reject time (default: \\texttt{0})"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :255
msgid "\\texttt{d}: (optional, long) Access duration (default: \\texttt{0})"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :255
msgid "\\texttt{pp}: (optional, string) Proxy package name"
msgstr ""

#. type: itemize
#: ./appendices/app_ops.tex
#: :255
msgid "\\texttt{pu}: (optional, integer) Proxy package uid"
msgstr ""

#. type: document
#: ./appendices/app_ops.tex
#: :258
msgid ""
"This definition can be found at "
"\\href{https://android.googlesource.com/platform/frameworks/base/+/master/services/core/java/com/android/server/appop/AppOpsService.java}{AppOpsService}."
msgstr ""

#. type: section{#2}
#: ./appendices/app_ops.tex
#: :263
msgid "Command Line Interface"
msgstr ""

#. type: document
#: ./appendices/app_ops.tex
#: :263
msgid ""
"\\texttt{appops} or \\texttt{cmd appops} (on latest versions) can be "
"accessible via ADB or root. This is an easier method to get or update any "
"operation for a package (provided the package name is known). The help page "
"of this command is self-explanatory:"
msgstr ""

#. type: Verbatim
#: main.tex:123
msgid ""
"AppOps service (appops) commands: help Print this help text.  start [--user "
"<USER_ID>] <PACKAGE | UID> <OP> Starts a given operation for a particular "
"application.  stop [--user <USER_ID>] <PACKAGE | UID> <OP> Stops a given "
"operation for a particular application.  set [--user <USER_ID>] <[--uid] "
"PACKAGE | UID> <OP> <MODE> Set the mode for a particular application and "
"operation.  get [--user <USER_ID>] <PACKAGE | UID> [<OP>] Return the mode "
"for a particular application and optional operation.  query-op [--user "
"<USER_ID>] <OP> [<MODE>] Print all packages that currently have the given op "
"in the given mode.  reset [--user <USER_ID>] [<PACKAGE>] Reset the given "
"application or all applications to default modes.  write-settings "
"Immediately write pending changes to storage.  read-settings Read the last "
"written settings, replacing current state in RAM.  options: <PACKAGE> an "
"Android package name or its UID if prefixed by --uid <OP> an AppOps "
"operation.  <MODE> one of allow, ignore, deny, or default <USER_ID> the user "
"id under which the package is installed. If --user is not specified, the "
"current user is assumed."
msgstr ""
